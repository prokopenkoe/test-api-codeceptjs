{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "id",
        "username",
        "firstName",
        "lastName",
        "email",
        "password",
        "phone",
        "userStatus"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                10000
            ]
        },
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "pettstore"
            ]
        },
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema",
            "examples": [
                "Василий"
            ]
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema",
            "examples": [
                "Юркин"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "vas@mail.ru"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "123123"
            ]
        },
        "phone": {
            "type": "string",
            "default": "",
            "title": "The phone Schema",
            "examples": [
                "+799999999999"
            ]
        },
        "userStatus": {
            "type": "integer",
            "default": 0,
            "title": "The userStatus Schema",
            "examples": [
                1
            ]
        }
    },
    "examples": [
        {
            "id": 10000,
            "username": "pettstore",
            "firstName": "Василий",
            "lastName": "Юркин",
            "email": "vas@mail.ru",
            "password": "123123",
            "phone": "+799999999999",
            "userStatus": 1
        }
    ]
}